    <animation>
        <type>rotate</type>
        <object-name>LHyoke</object-name>
        <object-name>LHyoke.column</object-name>
        <object-name>RHyoke</object-name>
        <object-name>RHyoke.column</object-name>
        <property>controls/flight/elevator</property>
        <factor>10</factor>
        <center>
            <x-m>-9.541</x-m>
            <y-m>0</y-m>
            <z-m>0.267</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>LHyoke</object-name>
        <property>controls/flight/aileron</property>
        <factor>30</factor>
        <center>
            <x-m>-9.460</x-m>
            <y-m>-0.570</y-m>
            <z-m>0.840</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>RHyoke</object-name>
