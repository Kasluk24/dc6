<!--   GEAR -->
   <animation>
        <type>select</type>
        <object-name>unsafe.on</object-name>
        <condition>
            <and>
                <less-than>
                    <property>gear/gear[0]/position-norm</property>
                    <value>1.0</value>
                </less-than>
                <greater-than>
                    <property>gear/gear[0]/position-norm</property>
                    <value>0.0</value>
                </greater-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>left.on</object-name>
        <condition>
            <equals>
                <property>gear/gear[1]/position-norm</property>
                <value>1.0</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>nose.on</object-name>
        <condition>
            <equals>
                <property>gear/gear[0]/position-norm</property>
                <value>1.0</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>right.on</object-name>
        <condition>
            <equals>
                <property>gear/gear[2]/position-norm</property>
                <value>1.0</value>
            </equals>
        </condition>
    </animation>
    
	<!-- Steeringwheel -->
	<!-- TODO: Add controls property for steering -->
	<animation>
		<type>pick</type>
		<object-name>NosewheelSteer</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>gear/gear[0]/steering-norm</property>
				<step>0.05</step>
				<max>1</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>gear/gear[0]/steering-norm</property>
				<step>-0.05</step>
				<min>-1</min>
			</binding>
		</action>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>NosewheelSteer</object-name>
		<property>gear/gear[0]/steering-norm</property>
		<factor>360</factor>
		<axis>
			<object-name>NosewheelSteer.axis</object-name>
		</axis>
	</animation>

